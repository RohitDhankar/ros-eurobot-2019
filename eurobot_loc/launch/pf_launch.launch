<?xml version="1.0"?>
<launch>
  <rosparam file="$(find eurobot_loc)/launch/secondary_robot_params.yaml" command="load" ns="secondary_robot"/>
  <param name = "field/color" value = "orange"/>
  <rosparam param="/use_sim_time">true</rosparam>
  <node pkg="rosbag" type="play" name="player" args="--clock /home/egorpristanskiy/.ros/robot.bag"/>
  <include file="$(find eurobot_sim)/launch/visualization.launch" />
  <group ns = "secondary_robot">
    <node pkg="eurobot_loc" name="particle_filter_node" type="particle_filter_node.py" respawn="true" output="screen"/>
  </group>
</launch>
