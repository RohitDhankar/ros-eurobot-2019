  <param name = "field/color" value = "orange"/>
  <rosparam param="/use_sim_time">true</rosparam>
  <node pkg="rosbag" type="play" name="player" args="--clock -s 35 /home/egorpristanskiy/.ros/2018-12-10-17-48-51.bag" output="screen"/>
